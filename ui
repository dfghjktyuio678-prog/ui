                    BorderColor3 = FromRGB(0, 0, 0),
                    Text = "125, 125, 125",
                    AnchorPoint = Vector2New(1, 1),
                    Size = UDim2New(0.44999998807907104, 0, 0, 20),
                    PlaceholderColor3 = FromRGB(178, 178, 178),
                    Position = UDim2New(1, -8, 1, -38),
                    BorderSizePixel = 0,
                    TextSize = 16,
                    BackgroundColor3 = FromRGB(23, 23, 29)
                })  Items["RGBInput"]:AddToTheme({TextColor3 = "Inactive Text", BackgroundColor3 = "Inline"})
                
