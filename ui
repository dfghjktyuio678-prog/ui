                Library:Connect(RunService.RenderStepped, function()
                    local MouseLocation = UserInputService:GetMouseLocation() 
                    Items["MouseImage"]:Tween(TweenInfo.new(0.14, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Position = UDim2New(0, MouseLocation.X, 0, MouseLocation.Y)})
                end)

                Items["SettingsButton"]:OnHover(function()
                    Items["SettingsButton"]:ChangeItemTheme({ImageColor3 = "Accent"})
                    Items["SettingsButton"]:Tween(nil, {ImageColor3 = Library.Theme.Accent, Rotation = -180})
